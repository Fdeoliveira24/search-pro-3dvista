      "3DModelObject": "./search-pro-non-mod/assets/3d-model-object-default.jpg",
      "default": "./search-pro-non-mod/assets/default-thumbnail.jpg"
    },
    "iconSettings": {
      "enableCustomIcons": true,
      "enableFontAwesome": false,
