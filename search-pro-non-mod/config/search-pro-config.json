        "Business": "fas fa-building",
        "3DHotspot": "fas fa-gamepad",
        "Container": "fas fa-window-restore",
        "3DModel": "fas fa-cube",
        "3DModelObject": "fas fa-wrench",
        "default": "fas fa-circle"
      },
      "showIconFor": {
        "panorama": true,
        "hotspot": true,
        "polygon": true,
        "video": true,
        "webframe": true,
        "image": true,
        "text": true,
        "projectedImage": true,
        "element": true,
        "business": true,
        "3dmodel": true,
        "3dhotspot": true,
        "3dmodelobject": true,
        "container": true,
        "other": true
